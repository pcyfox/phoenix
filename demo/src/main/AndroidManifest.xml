<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guoxiaoxing.phoenix.demo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="design_width_in_dp"
            android:value="720" />
        <meta-data
            android:name="design_height_in_dp"
            android:value="1280" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".picture.PictureDemoActivity" />
        <activity android:name=".video.VideoDemoActivity" />
        <activity android:name=".camera.CameraDemoActivity" />
        <activity android:name=".MainFragmentActivity">
        </activity>
    </application>

</manifest>